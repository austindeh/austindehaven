<a href="<%= locals.href %>" class="project">
  <img class="project-img" src="<%= locals.src %>">
  <canvas class="project-canvas"></canvas>
  <div class="project-details">
    <span class="project-number">0<%= locals.index %> / 0<%= locals.total %></span>
    <span class="project-title"><%= locals.title %></span>
    <span class="project-year"><%= locals.year %></span>
  </div>
</a>
